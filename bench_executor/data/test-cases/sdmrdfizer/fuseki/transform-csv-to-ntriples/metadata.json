{
    "@id": "http://dylanvanassche.be/test-cases/sdmrdfizer/fuseki/transform-csv-to-ntriples",
    "name": "SDM-RDFizer: Fuseki: CSV file",
    "description": "Transform student.csv into N-Triples with SDM-RDFizer with Fuseki Triplestore",
    "steps": [
        {
            "@id": "http://dylanvanassche.be/test-cases/sdmrdfizer/fuseki/transform-csv-to-ntriples#step1",
            "name": "Transform CSV into RDF",
            "resource": "SDMRDFizer",
            "command": "execute_mapping",
            "parameters": {
                "mapping_file": "mapping.rml.ttl",
                "output_file": "out.nt",
                "serialization": "nquads"
            }
        },
        {
            "@id": "http://dylanvanassche.be/test-cases/sdmrdfizer/fuseki/transform-csv-to-ntriples#step2",
            "name": "Load RDF",
            "resource": "Fuseki",
            "command": "load",
            "parameters": {
                "rdf_file": "out.nt"
            }
        },
        {
            "@id": "http://dylanvanassche.be/test-cases/sdmrdfizer/fuseki/transform-csv-to-ntriples#step3",
            "name": "Execute SPARQL query",
            "resource": "Query",
            "command": "execute_and_save",
            "parameters": {
                "query": "PREFIX foaf: <http://xmlns.com/foaf/0.1/> CONSTRUCT WHERE { ?s foaf:name ?o1 . }",
                "sparql_endpoint": "http://localhost:3030/ds",
                "results_file": "results.nt"
            }
        }
    ]
}
