{
    "@id": "http://dylanvanassche.be/test-cases/rmlmapper/virtuoso/transform-mysql-to-ntriples",
    "name": "RMLMapper: Virtuoso: MySQL RDB",
    "description": "Transform MySQL database into N-Triples with RMLMapper",
    "steps": [
        {
            "@id": "http://dylanvanassche.be/test-cases/rmlmapper/virtuoso/transform-mysql-to-ntriples#step1",
            "name": "Load RDB",
            "resource": "MySQL",
            "command": "load",
            "parameters": {
                "csv_file": "student.csv",
                "table": "student"
            }
        },
        {
            "@id": "http://dylanvanassche.be/test-cases/rmlmapper/virtuoso/transform-mysql-to-ntriples#step2",
            "name": "Transform MySQL into RDF",
            "resource": "RMLMapper",
            "command": "execute_mapping",
            "parameters": {
                "mapping_file": "mapping.r2rml.ttl",
                "output_file": "out.nt",
                "serialization": "nquads",
                "rdb_host": "MySQL",
                "rdb_port": "3306",
                "rdb_username": "root",
                "rdb_password": "root",
                "rdb_type": "MySQL",
                "rdb_name": "db"
            }
        },
        {
            "@id": "http://dylanvanassche.be/test-cases/rmlmapper/virtuoso/transform-mysql-to-ntriples#step3",
            "name": "Load RDF",
            "resource": "Virtuoso",
            "command": "load",
            "parameters": {
                "rdf_file": "out.nt"
            }
        },
        {
            "@id": "http://dylanvanassche.be/test-cases/rmlmapper/virtuoso/transform-mysql-to-ntriples#step4",
            "name": "Execute SPARQL query",
            "resource": "Query",
            "command": "execute_and_save",
            "parameters": {
                "query": "PREFIX foaf: <http://xmlns.com/foaf/0.1/> CONSTRUCT WHERE { ?s foaf:name ?o1 . }",
                "sparql_endpoint": "http://localhost:8890/sparql",
                "results_file": "results.nt"
            }
        }
    ]
}
